{
  "name": "dynasty-website-admin",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "env-cmd -f .env.development vite",
    "run:staging": "env-cmd -f .env.staging vite",
    "run:prod": "env-cmd -f .env.production vite",
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "lint:fix": "eslint --fix src --ext ts,tsx",
    "prettier": "prettier --check \"src/**/(*.tsx|*.ts|*.css|*.scss)\"",
    "format": "prettier --write \"src/**/(*.tsx|*.ts|*.css|*.scss)\"",
    "prepare": "husky install",
    "ci:tsc": "tsc --noEmit",
    "ci:eslint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "ci:highvoltage": "highvoltage ci --failOnErrors",
    "ci:test": "jest --ci"
  },
  "author": {
    "name": "Tan Pham Developer",
    "email": "phamvantan1311@gmail.com"
  },
  "dependencies": {
    "@ant-design/icons": "^5.1.4",
    "@internationalized/date": "^3.5.2",
    "@nextui-org/react": "^2.3.6",
    "@react-aria/i18n": "^3.10.2",
    "@react-pdf/renderer": "^3.1.14",
    "@reduxjs/toolkit": "^1.9.5",
    "@tanstack/react-query": "^4.29.25",
    "@types/lodash": "^4.14.196",
    "@types/qs": "^6.9.7",
    "add": "^2.0.6",
    "antd": "^4.24.10",
    "apexcharts": "^3.41.0",
    "autoprefixer": "^10.4.14",
    "axios": "^1.4.0",
    "chart.js": "^3.3.2",
    "dayjs": "^1.11.10",
    "env-cmd": "^10.1.0",
    "framer-motion": "^10.16.16",
    "html2canvas": "^1.4.1",
    "jspdf": "^2.5.1",
    "jsvectormap": "^1.5.3",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "notistack": "^3.0.1",
    "prettier-plugin-tailwindcss": "^0.4.0",
    "qs": "^6.11.2",
    "react": "^18.2.0",
    "react-apexcharts": "^1.4.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-chartjs-2": "^3.0.3",
