    "eslint-plugin-react-refresh": "^0.4.1",
    "husky": "^8.0.3",
    "lint-staged": ">=10",
    "prettier": "^3.0.0",
    "typescript": "^5.0.2",
    "vite": "^4.4.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint --cache --fix"
  }
}
